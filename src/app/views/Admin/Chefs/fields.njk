<div class="item">
  <label for="avatar_url">Avatar</label>
  <input type="URL" name="avatar_url" placeholder="https://" value="{{chef.avatar_url}}">
</div>

<div class="item">
  <label for="name">Nome</label>
  <input type="text" name="name" placeholder="" value="{{chef.name}}">
</div>

<div class="item">
  <label for="description">Descrição</label>
  <textarea name="description" cols="30" rows="10">{{chef.description}}</textarea>
</div>